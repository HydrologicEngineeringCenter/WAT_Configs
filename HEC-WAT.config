javapath ../javaRunTime64/bin/javaw.exe
showConsole true
mainclass hec2.wat.WAT
appname HEC-WAT
consoleTitle HEC-WAT Version Dev Console

#:------------------------------------------------------:
#: Memory parameters                                    :
#:------------------------------------------------------:
vmparam -Xmx6g

#:------------------------------------------------------:
#: Log Definitions                                      :
#:------------------------------------------------------:
vmparam -DshowMemoryUsage=true
vmparam -DUseEmbeddedDssVue=false
vmparam -DLOGFILE=$APPDATA\HEC\HEC-WAT\dev\logs\HEC-WAT.log
vmparam -DDSSLOGFILE=$APPDATA\HEC\HEC-WAT\dev\logs\HEC-WAT_DSS.log
vmparam -DCWMS_HOME=.
vmparam -Djava.library.path=lib
vmparam -DstatePlane.directory=config
vmparam -Djava.security.policy=config/.java.policy
vmparam -Dpython.path=jar\sys\jythonLib.jar\lib;jar\sys\jythonutils.jar
vmparam -Dpython.home=$TMP
vmparam -Djava.util.logging.config.file=./config/properties/logging.properties
vmparam -DnoServerSideSecurity=true
vmparam -Dcequal.tdg.fix=true
vmparam -Djavax.xml.parsers.SAXParserFactory=com.sun.org.apache.xerces.internal.jaxp.SAXParserFactoryImpl

#define the default folder for plugins
vmparam -DPLUGINS=jar/ext

# for the exceedance probability file
vmparam -DHSEPFILE=HSSynthEP.xml
vmparam -DUseWindowsLnF=true
vmparam -Dhttp.agent="Mozilla/4.0 (compatible; MSIE 6.0; Windows NT 5.0)"
#MediaLib is used to help load the internet maps faster
vmparam -Dcom.sun.media.jai.disableMediaLib=true

# +-----------------------------------------------------+
# |                                                     |
# | Specify the class path.                             |
# |                                                     |
# +-----------------------------------------------------+
addpath classes
addjars jar
#addjar jar/geotools-crs-1.0.jar
#addjar jar/hec-v6.0.jar
#addjar jar/hecnf-v3.1.jar
#addjar jar/images.jar
#addjar jar/lookup.jar
#addjar jar/mrSidReader.jar
#addjar jar/rma-v6.0.jar
#addjar jar/rmadev-v3.1.jar
#addjar jar/wat.jar
addjars jar/sys64
addjars jar/ext
addjars jar/sys
addjars jar/sys/excel
addjars jar/sys/geotools
addjars jar/sys/ignite
addjar ../apps/HEC-HMS/hms.jar

# Force standard VM settings for all applications
#turn off java 3d, on minimized remote desktops it causes massive CPU usage.
vmparam -Dsun.java2d.d3d=false 
vmparam -Xverify:none

#:------------------------------------------------------:
#: Disables VolitileImages in the MapPanel solving      :
#: some bad graphical glitches.			                    :
#:------------------------------------------------------:
vmparam -DMapPanel.NoVolatileImage=true

#:------------------------------------------------------:
#: WAT Application Settings                             :
#:------------------------------------------------------:
# allows the javastarter to restart the WAT during a compute with errors
keepalive true

#for master node: uncomment the following line if this is master node
addjar jar/rmiServer.jar
#turn off server side access control list checking for file manager 
#needed for gridgain compute master which starts a file manager
#for master node: uncomment the following line if this is master node
vmparam -DnoServerSideSecurity=true

#Example of properties that affect DC
#reduce amount of logging done by Ignite
#vmparam -DIGNITE_QUIET=true
#turn off performance suggestions from Ignite
#vmparam -DIGNITE_PERFORMANCE_SUGGESTIONS_DISABLED=true
#Its possible to override the default IgniteConfiguration used in WAT by providing an alternate configuration file.
#This would be needed at Amazon to configure a different Discovery mechanism.  Command to launch compute nodes would also change.
vmparam -DGridGainComputePlugin.configFile="C:\Programs\DC\default-config.xml"
#Also possible to override the workingDir. Specified directory needs to be big enough to hold study and results.
#Default working dir is %temp%\GridGainWorkingDir\ and usually doesn't need to be changed.  
#vmparam -DGridGainComputePlugin.workingDir="C:\temp\IgniteWorkingDir"
#Number of times to retry a job if it returns false without throwing an exception. Default 0 = Don't retry. -1 = Retry on every node.
#for master node: uncomment the following line if this is master node and you wish to retry compute jobs if/when they fail
#vmparam -DGridGainComputePlugin.retryCount=3
#Save successfully merged toMerge dss files (default = false)
#vmparam -DGridGainComputePlugin.saveMerge=false
#Distribute work by realization (default)
#vmparam -DsplitBy=realization
#Distribute work by lifecycle
#vmparam -DsplitBy=lifecycle
#A large DC can involve thousands of RmiFile objects. Interning the owner can save some memory.
#vmparam -DRmiFile.internOwner=true
vmparam -DMerge.IntegrityLevel=0

#logging to HEC-WAT\config\properties
#vmparam -Djava.util.logging.config.file=./config/properties/logging.properties
#Override check for convergence variable on compute
vmparam -DNoComputeConvergenceCheck=true
